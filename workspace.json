      }
    },
    "memo-app-e2e": {
      "root": "apps/memo-app-e2e",
      "sourceRoot": "apps/memo-app-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/memo-app-e2e/cypress.json",
            "tsConfig": "apps/memo-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "memo-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "memo-app:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/memo-app-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
